{
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#696969"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"string.template",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#bfbf91"
			}
		},
		{
			"name": "Variable other readwrite",
			"scope": "variable.other.readwrite",
			"settings": {
				"foreground": "#CCC"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#9C88BD"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other",
				"variable.other.constant.ruby"
			],
			"settings": {
				"foreground": "#9C88BD"
			}
		},
		{
			"name": "Variable language",
			"scope": [
				"variable.language",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#C4873D"
			}
		},
		{
			"name": "Keyword",
			"scope": [ 
				"keyword",
				"punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9C88BD"
			}
		},
		{
			"name": "Storage type function",
			"scope": "storage.type.function",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75c2d0"
			}
		},
		{
			"name": "Storage type class",
			"scope": "storage.type.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75c2d0"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a4b551"
			}
		}, 
		{
			"name": "Inherited class",
			"scope": [
				"support.class",
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a4b551"
			}
		},
		{
			"name": "Function name",
			"scope": [
				"meta.definition.function",
				"meta.function.php",
				"meta.definition.method"
			],
			"settings": {
				"foreground": "#a4b551"
			}
		},
		{
			"name": "Function call",
			"scope": "meta.function-call",
			"settings": {
				"foreground": "#6693BD"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C4873D"
			}
		},
		{
			"name": "Tag name",
			"scope": [
				"entity.name.tag",
				"entity.other.attribute-name.pseudo-element"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Entity CSS pseudo",
			"scope": "entity.other.attribute-name.pseudo-class",
			"settings": {
				"fontStyle": "",
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Support CSS constant / transform",
			"scope": [
				"support.constant.property-value",
				"support.function.transform.css"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#9C88BD"
			}
		},


		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#809C7D"
			}
		},
		{
			"name": "Support function",
			"scope": "support.function",
			"settings": {
				"foreground": "#6693BD"
			}
		},
		{
			"name": "Support function funchand",
			"scope": [
				"support.function.funchand",
				"entity.name.function.ruby"
			],
			"settings": {
				"foreground": "#75c2d0"
			}
		},
		{
			"name": "Support type",
			"scope": [
				"support.type",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#75c2d0",
				"fontStyle": "italic"
			}
		},	
		{
			"name": "Support variable",
			"scope": [
				"support.variable",
				"variable.other"
			],
			"settings": {
				"foreground": "#CCC"
			}
		},
		{
			"name": "JSON key",
			"scope": "support.type.property-name.json",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "JSON string punctuation",
			"scope": "punctuation.separator.dictionary.key-value",
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Punctuation tag",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#416194"
			}
		},
		{
			"name": "Punctuation arrow",
			"scope": "punctuation.accessor.arrow",
			"settings": {
				"foreground": "#d74385"
			}
		},
		{
			"name": "Punctuation string template",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#6693BD"
			}
		},
		{
			"name": "Punctuation definition",
			"scope": [
				"punctuation.definition",
				"punctuation.separator.delimiter",
				"punctuation.section",
				"punctuation.accessor",
				"meta.brace.round"
			],
			"settings": {
				"foreground": "#CCC"
			}
		},
		{
			"name": "Punctuation jsdoc",
			"scope": [
				"punctuation.definition.block.tag",
				"storage.type.class.jsdoc"
			],
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Entity name jsdoc",
			"scope": "entity.name.type.instance.jsdoc",
			"settings": {
				"foreground": "#bfbf91"
			}
		},

		{
			"name": "Markdown section title hash",
			"scope": [
				"punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Markdown section title",
			"scope": "entity.name.section.markdown",
			"settings": {
				"foreground": "#B2C472"
			}
		},
		{
			"name": "Markdown link title",
			"scope": [
				"string.other.link.title.markdown",
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown",
			],
			"settings": {
				"foreground": "#bfbf91"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.underline",
				"punctuation.definition.metadata.markdown"
			],
			"settings": {
				"foreground": "#75C2D0"
			}
		},
		{
			"name": "Markup list",
			"scope": [
				"markup.list"
			],
			"settings": {
				"foreground": "#A8A1B3"
			}
		},
		{
			"name": "Markup separators",
			"scope": [
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "#999"
			}
		},
		{
			"name": "Markup blockquote",
			"scope": [
				"punctuation.definition.blockquote"
			],
			"settings": {
				"foreground": "#bbb"
			}
		},
		{
			"name": "Markup code",
			"scope": [
				"punctuation.definition.raw.markdown",
				"markup.inline.raw.string.markdown",
			],
			"settings": {
				"foreground": "#8C9CAD"
			}
		},
		{
			"name": "Markup bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		}
	],
	"colors": {
		"editor.background": "#181D20",
		"editorCursor.foreground": "#F8F8F0",
		"editor.foreground": "#CCC",
		"editorWhitespace.foreground": "#3B3A32",
		"editor.lineHighlightBackground": "#22292e48",
		"editor.selectionBackground": "#6e6e6e46",
		"statusBar.background": "#111", 
		"statusBar.noFolderBackground": "#111",
		"statusBar.foreground": "#999",
		"statusBarItem.hoverBackground": "#222",
		"editorIndentGuide.background": "#35353580",
	},
	"name": "Monokai Neo"
}