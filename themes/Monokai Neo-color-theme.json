{
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#696969"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted.single",
				"string.quoted.double",
				"string.template"
			],
			"settings": {
				"foreground": "#bfbf91"
			}
		},
		{
			"name": "Variable other readwrite",
			"scope": "variable.other.readwrite",
			"settings": {
				"foreground": "#BBBBBB"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#9C88BD"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#9C88BD"
			}
		},
		{
			"name": "Variable language",
			"scope": [
				"variable.language",
				"variable.language.this"
			],
			"settings": {
				"foreground": "#C4873D"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9C88BD"
			}
		},
		{
			"name": "Storage type function",
			"scope": "storage.type.function",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75c2d0"
			}
		},
		{
			"name": "Storage type class",
			"scope": "storage.type.class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#75c2d0"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#a4b551"
			}
		},
		{
			"name": "Inherited class",
			"scope": [
				"support.class",
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#a4b551"
			}
		},
		{
			"name": "Function name",
			"scope": [
				"meta.definition.function"
			],
			"settings": {
				"foreground": "#a4b551"
			}
		},
		{
			"name": "Function call",
			"scope": "meta.function-call",
			"settings": {
				"foreground": "#6693BD"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C4873D"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#BA3E70"
			}
