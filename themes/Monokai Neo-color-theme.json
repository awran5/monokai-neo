		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#809C7D"
			}
		},
		{
			"name": "Support function",
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "#6693BD"
			}
		},
		{
			"name": "Support variable",
			"scope": [
				"support.variable",
				"variable.other",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#BBBBBB"
			}
		},
		{
			"name": "JSON key",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#75c2d0"
			}
		},
		{
			"name": "JSON String",
			"scope": "string.quoted.double.json",
			"settings": {
				"foreground": "#BBBBBB"
			}
		},
		{
			"name": "JSON string punctuation",
			"scope": "punctuation.separator.dictionary.key-value",
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Punctuation tag",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#416194"
			}
		},
		{
			"name": "Punctuation arrow",
			"scope": "punctuation.accessor.arrow",
			"settings": {
				"foreground": "#d74385"
			}
		},
		{
			"name": "Punctuation string template",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end"
			],
			"settings": {
				"foreground": "#6693BD"
			}
		},
		{
			"name": "Punctuation dollar",
			"scope": "punctuation.dollar",
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Markdown section title hash",
			"scope": [
				"punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#BA3E70"
			}
		},
		{
			"name": "Markdown section title",
			"scope": "entity.name.section.markdown",
			"settings": {
				"foreground": "#E1E1E1"
			}
		},
		{
			"name": "Markdown paragraph",
			"scope": "meta.paragraph.markdown",
			"settings": {
				"foreground": "#999999"
			}
		},
		{
			"name": "Markdown link title",
			"scope": [
				"string.other.link.title.markdown",
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown",
			],
			"settings": {
				"foreground": "#bfbf91"
			}
		},
		{
			"name": "Markdown link",
			"scope": [
				"markup.underline",
				"punctuation.definition.metadata.markdown"
			],
			"settings": {
				"foreground": "#75C2D0"
			}
		}
	],
	"colors": {
		"editor.background": "#181D20",
		"editorCursor.foreground": "#F8F8F0",
		"editor.foreground": "#BBBBBB",
		"editorWhitespace.foreground": "#3B3A32",
		"editor.lineHighlightBackground": "#333333",
		"editor.selectionBackground": "#333333",
		"statusBar.background": "#111111", 
		"statusBar.noFolderBackground": "#111111",
		"statusBar.foreground": "#999999",
		"statusBarItem.hoverBackground": "#222222",
		"editorIndentGuide.background": "#35353580",
	},
	"name": "Monokai Neo"
}